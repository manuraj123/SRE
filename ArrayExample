public class ArrayExample {
    public static void main(String[] args) {
        int[] numbers = { 2, 4, 6, 8, 10 };
        System.out.println("Elements of the array:");
        for (int i = 0; i < numbers.length; i++) {
            System.out.println("Element " + i + ": " + numbers[i]);
        }
    }
}
