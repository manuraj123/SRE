public class ForEachLoopExample {
    public static void main(String[] args) {
        int[] numbers = {1, 2, 3, 4, 5};


        for (int num : numbers) {
            System.out.println(num);
        }

        String[] fruits = {"Apple", "Banana", "Orange"};

        for (String fruit : fruits) {
            System.out.println(fruit);
        }
    }
}
